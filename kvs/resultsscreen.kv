<FoodbankIcon>
    size: 100, 100
    size_hint_y: None
    ripple_behavior: True

<ResultsScreen>
    name: "results_screen"
    BoxLayout:
        orientation: "vertical"
        MDToolbar:
            title: "Results"
            left_action_items: [['arrow-left', lambda x: root.go_back()]]
            right_action_items: [['magnify', lambda x: root.go_back()]]

        ScrollView:
            MDBoxLayout:
                id: bank_icons
                adaptive_height: True
                orientation: 'vertical'
                spacing: '30dp'
                padding: '10dp'